pipeline {
  agent {
    dockerfile true
  }

  stages {
    stage('checkout') {
      steps {
        checkout scm

      }
    }
    stage('docker Build') {
      agent {
        dockerfile true
      }
      steps {

        sh sudo docker build - t vaishnavipasupulati / myfirstapp.

        sh echo "running"
        sh sudo docker run - itd - p 8888: 5000--name catgif1 vaishnavipasupulati / myfirstapp

        sh echo "execute"
        sh sudo docker exec container - it sh
      }

    }
  }
}
